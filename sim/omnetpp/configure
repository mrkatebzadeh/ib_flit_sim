        ;;
    macosx)
        cp -r ide/macosx/* ide
        echo osgi.instance.area.default=../../../../samples >>ide/configuration/config.ini
        ;;
    *)
        echo IDE is supported only on: Linux 32/64bit, Windows 32bit, MacOS X Intel. Not configured.
        ;;
    esac
  fi
fi

