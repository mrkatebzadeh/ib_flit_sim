   { echo "$as_me:$LINENO: result: -fpic" >&5
echo "${ECHO_T}-fpic" >&6; }
   fpic="-fpic"
fi
if test "$fpic" = "x"; then
   { echo "$as_me:$LINENO: result: failure" >&5
echo "${ECHO_T}failure" >&6; }
   { echo "$as_me:$LINENO: WARNING: COULD NOT FIGURE OUT HOW TO BUILD SHARED LIBRARY -- reverting to static libs" >&5
