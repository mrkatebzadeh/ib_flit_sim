%description:
check empty/NULL string, empty/NULL delimiters

%activity:

const char *token;

{
    cStringTokenizer tok("");
    while ((token = tok.nextToken())!=NULL)
        EV << ":" << token;
    EV << ".\n";
}

{
    cStringTokenizer tok(NULL);
    while ((token = tok.nextToken())!=NULL)
        EV << ":" << token;
    EV << ".\n";
}

{
    cStringTokenizer tok("bla bla", "");
    while ((token = tok.nextToken())!=NULL)
        EV << ":" << token;
    EV << ".\n";
}

{
    cStringTokenizer tok("bla bla", NULL);
    while ((token = tok.nextToken())!=NULL)
        EV << ":" << token;
    EV << ".\n";
}


%contains: stdout
.
.
:bla:bla.
:bla:bla.
